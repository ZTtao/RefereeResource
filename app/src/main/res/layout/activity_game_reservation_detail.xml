<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="time"/>
    <TextView
        android:id="@+id/tv_activity_game_reservation_detail_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000-10-10 10:10"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="address"/>
    <TextView
        android:id="@+id/tv_activity_game_reservation_detail_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="team"/>
    <TextView
        android:id="@+id/tv_activity_game_reservation_detail_team"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level"/>
    <TextView
        android:id="@+id/tv_activity_game_reservation_detail_level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/btn_activity_game_reservation_detail_reply"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reply"/>
</LinearLayout>